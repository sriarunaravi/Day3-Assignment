package Automation;

public class ContinueOddNumber {
	 public static void main(String[] args) {
		 System.out.println("The Odd Numbers Are:");
	        for (int i = 1; i <= 50; i++) {	           	  
	            if (i % 2 == 0) {
	                continue;  
	            }
	           	            System.out.println(i);
	        }
	    }
	}
